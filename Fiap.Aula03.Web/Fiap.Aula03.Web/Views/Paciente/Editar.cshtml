@model Paciente

@{
    ViewData["Title"] = "Editar";
}

<h1>Editar</h1>

<form asp-action="editar" asp-controller="paciente">
    <input asp-for="Codigo"/>
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="DataEntrada"></label>
        <input asp-for="DataEntrada" class="form-control"/>
    </div>
    <div class="form-group">
        <label asp-for="Temperatura"></label>
        <input  asp-for="Temperatura" class="form-control"/>
    </div>
    <div class="form-group">
        <input asp-for="Covid"/>
        <label asp-for="Covid"></label>
    </div>
    <input type="submit" value="Editar" class="btn btn-primary"/>
</form>

